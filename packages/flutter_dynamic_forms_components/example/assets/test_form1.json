                        }
                    ]
                }
            ]
        },
        {
            "@name": "formGroup",
            "id": "formgroup4",
            "name": "Form section 4",
            "children": [
                {
                    "@name": "slider",
                    "id": "Form Slider",
                    "value": "15.0",
                    "minValue": "0.0",
                    "maxValue": "100.0"
                }
            ]
        },
        {
            "@name": "formGroup",
            "id": "formgroup5",
            "name": "Form section 5",
            "children": [
                {
                    "@name": "date",
                    "id": "Form Date",
                    "firsDate": "2010-02-27",
                    "lastDate": "2030-02-27",
                    "format": "yyyy-MM-dd",
                    "initialDate": "2019-10-20"
                },
                {
                    "@name": "dateRange",
                    "id": "Form Date Range",
                    "minDate": "2010-02-27",
                    "maxDate": "2030-02-27",
