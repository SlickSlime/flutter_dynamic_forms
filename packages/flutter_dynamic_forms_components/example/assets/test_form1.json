                    "id": "formgroup3",
                    "name": "Form section 3",
                    "children": [
                        {
                            "@name": "dropdownButton",
                            "id": "dropdownButton1",
                            "label": "Countries",
                            "value": "CD",
                            "choices": [

                                {
                                    "@name": "dropdownOption",
                                    "label": "Argentina",
                                    "value": "AR"
                                },
                                {
                                    "@name": "dropdownOption",
                                    "label": "Belarus",
                                    "value": "BY"
                                },
                                {
                                    "@name": "dropdownOption",
                                    "label": "Brazil",
                                    "value": "BR"
                                },
                                {
                                    "@name": "dropdownOption",
                                    "label": "The democratic republic of the Congo",
                                    "value": "CD"
                                },
                                {
                                    "@name": "dropdownOption",
                                    "label": "Falkland Islands",
                                    "value": "FK"
                                },
                                {
                                    "@name": "dropdownOption",
                                    "label": "Georgia",
                                    "value": "GE"
                                },
                                {
                                    "@name": "dropdownOption",
                                    "label": "Guinea",
                                    "value": "GN"
                                }
