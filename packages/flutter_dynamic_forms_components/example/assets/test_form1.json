{
    "@name": "form",
    "id": "form1",
    "children": [
        {
            "@name": "form",
            "id": "ncuDigiunivSetupForm",
            "children": [
                {
                    "@name": "label",
                    "id": "title",
                    "value": "Setup Attendance",
                    "color": "0xFFF000000",
                    "fontSize": "24",
                    "fontWeight": "w500"
                },
                {
                    "@name": "label",
                    "id": "description",
                    "fontSize": "16",
                    "value": "Add your ERP username and password to track your Attendance on Collegium.\n\nWhen you give permission, our bots automatically and securely sign-in to ERP to periodically fetch your attendance."
                },
                {
                    "@name": "textField",
                    "id": "username",
                    "label": "ERP Username",
                    "value": "",
                    "validations": [
                        {
                            "@name": "requiredValidation",
                            "message": "Username is required"
                        }
                    ]
                },
                {
                    "@name": "textField",
                    "id": "password",
                    "label": "ERP Password",
                    "obscureText": "true",
                    "validations": [
                        {
                            "@name": "requiredValidation",
                            "message": "Password is required"
                        }
                    ]
                },
                {
                    "@name": "checkBox",
                    "id": "termsConditionCheckBox",
                    "value": "false",
                    "label": "I accept all the terms and conditions."
                }
            ]
        },
        {
            "@name": "formGroup",
            "id": "formGroup1",
            "name": "Form section 1",
            "children": [
                {
                    "@name": "checkBox",
                    "id": "hideWelcomeCheckBox",
                    "value": "false",
                    "label": "Hide welcome message"
                },
                {
